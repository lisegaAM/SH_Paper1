FoamFile
{
    version     3.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}

application     sonicFoam;

startFrom       startTime;

startTime       0.5;

stopAt          endTime;

endTime         0.7;

deltaT          0.00025;

writeControl    runTime;

writeInterval   0.01;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression    off;

timeFormat      general;

timePrecision   6;

runTimeModifiable   true;

functions
{
    rhofunc
    {
        type                writeRegisteredObject;
        functionObjectLibs  ("libIOFunctionObjects.so");
        outputControl       outputTime;
        outputInterval      1;
        objectNames
        (
            "rho"
            "phi"
        );
    }

